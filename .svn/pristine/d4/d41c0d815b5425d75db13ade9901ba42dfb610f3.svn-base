package org.l2jmobius.gameserver.model.quest;

/**
 * @author Magik
 */
public enum QuestDialogType
{
	NONE(0),
	START(1),
	ACCEPT(2),
	COMPLETE(3),
	END(4),
	AGAIN(5);
	
	private final int _id;
	
	QuestDialogType(int id)
	{
		_id = id;
	}
	
	public int getId()
	{
		return _id;
	}
}
