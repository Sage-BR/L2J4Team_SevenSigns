package org.l2jmobius.gameserver.model.quest.newquestdata;

import java.util.List;

import org.l2jmobius.gameserver.model.holders.ItemHolder;

/**
 * @author Magik
 */
public class NewQuestReward
{
	private final long _exp;
	private final long _sp;
	private final int _level;
	private final List<ItemHolder> _items;
	
	public NewQuestReward(long exp, long sp, int level, List<ItemHolder> items)
	{
		_exp = exp;
		_sp = sp;
		_level = level;
		_items = items;
	}
	
	public long getExp()
	{
		return _exp;
	}
	
	public long getSp()
	{
		return _sp;
	}
	
	public int getLevel()
	{
		return _level;
	}
	
	public List<ItemHolder> getItems()
	{
		return _items;
	}
}
